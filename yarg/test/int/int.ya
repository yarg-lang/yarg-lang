fun tdf() {
    const td =
    [
        [13421772800000, 13421772800000],
        [13421772800700, 444],
        [-13421772800700, 444],
        [13421772800700, -444],
        [-13421772800700, -444],
        [9223372036854775807, 9223372036854775807],
        [9223372036854775807, 9223372036854775806],
        [9223372036854775806, 9223372036854775807],
        [9223372036854775807, 1],
        [9223372036854775806, 1],
        [9223372036854775807, 11],
        [9223372036854775806, 16],
        [9223372036854775806, 21],
        [9223372036854775806, 65536],
        [9223372036854775806, 4294967296]
    ];

    for (var x = 0; x < 15; x = x + 1)
    {
        var a = int(td[x][0]);
        var b = int(td[x][1]);
        var q = a / b;
        var r = a % b;

        var t = int(int64(td[x][0]) / int64(td[x][1]));
        var t2 = int(int64(td[x][0]) % int64(td[x][1]));

        print q == t; // expect: true
        print r == t2; // expect: true
    }
}
