fun af() {
    var x = int(10);
    x = x + int(-10);
    print x; // expect: 0

    x = x + int(10) + int(10);
    print x; // expect: 20

    var i8 = int8(-123);
    var u8 = uint8(200);
    var i16 = int16(-20000);
    var u16 = uint16(50000);
    var i32 = int32(-2000000000);
    var u32 = uint32(0d3000000000);
    var i64 = int64(-int(5000000000000000000));
    var u64 = uint64(0d10000000000000000000);
    print i8; // expect: -123
    print u8; // expect: 200
    print i16; // expect: -20000
    print u16; // expect: 50000
    print i32; // expect: -2000000000
    print u32; // expect: 3000000000
    print i64; // expect: -5000000000000000000
    print u64; // expect: 10000000000000000000

    var a = int(i8);
    var b = int(u8);
    var s = int(i16);
    var r = int(u16);
    var t = int(i32);
    var d = int(u32);
    var q = int(i64);
    var p = int(u64);
    print a; // expect: -123
    print b; // expect: 200
    print s; // expect: -20000
    print r; // expect: 50000
    print t; // expect: -2000000000
    print d; // expect: 3000000000
    print q; // expect: -5000000000000000000
    print p; // expect: 10000000000000000000

    var t2 = a + b + s + r + t + d + q + p;
    print t2; // expect: 5000000001000030077
}
af();

var tenToTheFour = int(10000);

fun bf() {
    var a = -0;
    print a; // expect: 0

    a = int(123);
    var b = int(4567);
    print a < b; // expect: true

    a = int(9223372036854775807);
    b = int(9223372036854775806);
    print a <= b; // expect: false

    a = 922337203685477580701;
    b = 922337203685477580700;
    print a < b; // expect: false

    a = 922337203685477580701;
    b = 922337203685477580700;
    print a == b; // expect: false

    a = int(0);
    b = int(-1);
    print a > b; // expect: true

    b = -b;
    print a >= b; // expect: false

    a = int(-2);
    b = int(-1);
    print a <= b; // expect: true

    b = int(2);
    print a < b; // expect: true

    a = int(123);
    b = int(4567);
    var r = a + b;
    var t = int(123+4567);
    print r == t; // expect: true

    a = int(1230000001);
    b = 45675555555;

    r = a + b;

    int64 t64 = int64(0d1230000001)+int64(0d45675555555);

    print r==int(t64); // expect: true

    a = int(9223372036854775807);
    b = int(9223372036854775806);
    r = a+b;
    a = r;
    b = int(3);
    r=a+b;
    print r==int(18446744073709551616); // expect: true
    print r; // expect: 18446744073709551616

    r = int(4567)-int(123);
    t = int(int32(4567)-int32(123));
    print r==t; // expect: true

    r = int(1230000001)-int(45675555555);
    t = int(int64(45675555555)-int64(1230000001));
    t = t * int(-1);
    print int(r)==t; // expect: true

    a = int(1);
    b = 2147483649;
    r = a * b;
    print r==b; // expect: true

    a = int(4567);
    b = int(123);
    r = a * b;
    t = int(4567*123);
    print r==t; // expect: true

    a = int(-4567);
    b = int(-123);
    r = a * b;
    print r==t; // expect: true

    b = int(123);
    r = a * b;
    t = int(-4567*123);
    print r==t; // expect: true

    a = int(1230000001);
    b = int(45675);
    r = a * b;
    t = int(int64(45675)*int64(1230000001));
    print r==t; // expect: true
    //var s = string(t);
    //print s == "56180250045675"; // expect: true
    print t; // expect: 56180250045675
    print int64(r) == int64(45675) * int64(1230000001); // expect: true

    var q = t / tenToTheFour;
    r = t % tenToTheFour;
    print q == int(5618025004); // expect: true
    print r == int(5675); // expect: true

    t = int(56);
    q = t / tenToTheFour;
    r = t % tenToTheFour;
    print q == int(0); // expect: true
    print r == int(56); // expect: true

    a = int("-12345");
    b = int(-12345);
    print a == b; // expect: true

    a = 13421772800000;
    b = 13421772800000;
    var c = a / b;
    print c == int(1); // expect: true

    a = 13421772800700;
    b = int(444);
    c = a / b;
    print c == 30229218019; // expect: true
}
bf();

